include header
h1 Админ - панель
h2 Информация о заказах
table
 tr
  th id
  th date
  th userId
  th goodsId
  th Goods Cost
  th Goods Amount
  th(class='text-danger') Итого
  th User
  th Phone
  th Address
 each val, index in order
  tr 
   td #{val['id']}
   td #{val['human_date']}
   td #{val['user_id']}
   td #{val['goods_id']}
   td #{val['goods_cost'].toFixed(2).replace(/\d(?=(\d{3})+\.)/g, '$& ')} руб
   td #{val['goods_amount']}
   td #{val['total'].toFixed(2).replace(/\d(?=(\d{3})+\.)/g, '$& ')} руб
   td #{val['user']}
   td #{val['phone']}
   td #{val['address']}